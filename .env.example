# Example .env file for PyDiagno
# Copy this file to .env and fill in your actual values

# LLM API Keys
PYDIAGNO_LLM__DEPLOYMENTS__0__API_KEY=your_openai_api_key_here
PYDIAGNO_LLM__DEPLOYMENTS__1__API_KEY=your_huggingface_api_key_here

# Event Bus Configuration
PYDIAGNO_EVENT_BUS__CONNECTION__PASSWORD=your_event_bus_password_here

# Database Credentials (if applicable)
PYDIAGNO_RAG__DATABASE__USERNAME=your_database_username
PYDIAGNO_RAG__DATABASE__PASSWORD=your_database_password

# Add any other sensitive configuration items here
